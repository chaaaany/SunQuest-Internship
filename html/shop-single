    <!--content query script-->
    <script>
        var ID;
        var nowLoc = window.location.href;
        var test;

        for(i=0; i < nowLoc.length; i++){
            if(nowLoc[i] == '?'){
                if(nowLoc.length - i == 6 ){
                    ID = nowLoc.slice(i+4,nowLoc.length);
                } 
                else if(nowLoc.length - i == 5){
                    ID = nowLoc.slice(length-1);
                }
                
                break;
            }
        }

        //test = nowLoc.slice(length-1);
        //test = nowLoc.length;
        let xhttp = new XMLHttpRequest();
        
        xhttp.onreadystatechange = function () {
            if(xhttp.readyState == 4 && xhttp.status == 200){
                
                jsonfunc(this.responseText); //this = xhttp
            }
        }

        xhttp.open("GET","https://www.cloveruniforms.com/product-json?id=" + ID, true);//
        xhttp.send();
        let json;
        
        function jsonfunc( jsonText ) {
            json = JSON.parse(jsonText);
            var cat = "";
            if(json[0].SEX == "W"){ cat += "Women's ";}
            else {cat += "Men's ";}
            if(json[0].P_CAT == "SC"){ cat += "Scrub";}
            else if(json[0].P_CAT == "ACC"){ cat += "Accessories";}
            else if(json[0].P_CAT == "LC"){ cat += "Lab Coat";}
            else if(json[0].P_CAT == "PG"){ cat += "Patient Gown";}
            else if(json[0].P_CAT == "OA"){ cat += "other apparel";}

            var path = "<img class=\"card-img img-fluid\" src=\"" + json[0].PATH + "\"alt=\"Card image cap\" id=\"product-detail\">";

            
            document.getElementById('path').innerHTML = path;
            document.getElementById('category').innerHTML = cat;
            document.getElementById('name').innerHTML = json[0].P_NAME;
            document.getElementById('careInfo').innerHTML =  "<br>" + json[0].FABRIC + "<br>" + json[0].CARE_INS + "<br>";
            document.getElementById('productInfo').innerHTML =  "<br>" + json[0].P_DETAIL + "<br>";
            document.getElementById('price').innerHTML = "$"+json[0].PRICE;
        }
    </script>
    


